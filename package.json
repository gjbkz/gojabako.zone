{
  "name": "@gjbkz/gojabako.zone",
  "homepage": "https://gojabako.zone/",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-s test:*",
    "test:type": "tsc --noEmit",
    "test:jest": "npx jest",
    "dev": "run-p dev:*",
    "dev:css": "typed-scss-modules app --watch --logLevel info",
    "dev:next": "NODE_OPTIONS='--experimental-loader=@nlib/tsm' next",
    "build": "run-s build:*",
    "build:pageList": "node --experimental-loader=@nlib/tsm src/build/pageList.mts",
    "build:css": "typed-scss-modules app --logLevel error",
    "build:next": "next build",
    "start": "NODE_ENV=production node server.mjs",
    "lint": "run-s lint:*",
    "lint:next": "next lint",
    "lint:eslint": "eslint src *.mjs"
  },
  "dependencies": {
    "@mdx-js/loader": "2.3.0",
    "@next/mdx": "13.4.19",
    "@nlib/eslint-config": "3.20.2",
    "@nlib/githooks": "0.2.0",
    "@nlib/lint-commit": "1.0.1",
    "@nlib/tsm": "0.1.1",
    "@nlib/typing": "1.1.0",
    "@types/hast": "3.0.0",
    "@types/node": "20.6.1",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "jose": "4.14.4",
    "lint-staged": "14.0.1",
    "meilisearch": "0.34.2",
    "next": "13.4.12",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "rehype-highlight": "7.0.0",
    "rehype-katex": "6.0.3",
    "rehype-parse": "9.0.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "sass": "1.67.0",
    "typed-scss-modules": "7.1.4",
    "typescript": "5.2.2",
    "unified": "11.0.3",
    "unist-util-visit": "5.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.*(m|c)+(j|t)s*(x)": "eslint"
  },
  "renovate": {
    "extends": [
      "github>nlibjs/renovate-config"
    ]
  }
}
