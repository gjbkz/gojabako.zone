{
  "name": "@gjbkz/gojabako.zone",
  "homepage": "https://gojabako.zone/",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-s test:*",
    "test:type": "tsc --noEmit",
    "test:unit": "tsm-test src",
    "dev": "run-p build:meta dev:*",
    "dev:css": "node --import=@nlib/tsm/register.mjs src/cli/scssTypes.mts --watch",
    "dev:stories": "node --import=@nlib/tsm/register.mjs src/cli/stories.mts --watch",
    "dev:next": "NODE_OPTIONS='--import=@nlib/tsm/register.mjs' next",
    "https": "run-p build:meta dev:css dev:stories https:*",
    "https:next": "node --import=@nlib/tsm/register.mjs server.mts",
    "build": "run-s build:*",
    "build:meta": "run-p build:meta:*",
    "build:meta:pageList": "node --import=@nlib/tsm/register.mjs src/cli/pageList.mts",
    "build:meta:fonts": "node --import=@nlib/tsm/register.mjs src/cli/fonts.mts",
    "build:meta:scssTypes": "node --import=@nlib/tsm/register.mjs src/cli/scssTypes.mts",
    "build:meta:stories": "node --import=@nlib/tsm/register.mjs src/cli/stories.mts",
    "build:next": "NODE_OPTIONS='--import=@nlib/tsm/register.mjs' next build",
    "start": "NODE_ENV=production NODE_OPTIONS='--import=@nlib/tsm/register.mjs' node server.mjs",
    "lint": "run-s lint:*",
    "lint:biome": "npx @biomejs/biome lint",
    "lint:css": "stylelint 'src/**/*.scss'"
  },
  "dependencies": {
    "@fontsource/noto-sans-jp": "5.1.0",
    "@mdx-js/loader": "3.0.1",
    "@next/mdx": "14.2.12",
    "@nlib/tsm": "0.1.3",
    "@nlib/typing": "1.1.0",
    "@storybook/csf": "0.1.11",
    "@types/kuromoji": "0.1.3",
    "@types/node": "22.5.5",
    "@types/react": "18.3.7",
    "acorn": "8.12.1",
    "chokidar": "3.6.0",
    "esbuild-loader": "4.2.2",
    "hast-util-from-html": "2.0.3",
    "hast-util-to-string": "3.0.0",
    "kuromoji": "0.1.2",
    "lint-staged": "15.2.10",
    "lowlight": "3.1.0",
    "mdast-util-from-markdown": "2.0.1",
    "mdast-util-to-hast": "13.2.0",
    "mdast-util-to-string": "4.0.0",
    "next": "14.2.10",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.47",
    "postcss-scss": "4.0.9",
    "postcss-selector-parser": "6.1.2",
    "prettier": "3.3.3",
    "recoil": "0.7.7",
    "rehype-highlight": "7.0.0",
    "rehype-katex": "7.0.1",
    "rehype-parse": "9.0.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "remark-math": "6.0.0",
    "sass": "1.79.1",
    "sharp": "0.33.5",
    "stylelint": "16.9.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.5.4",
    "unist-util-find": "3.0.0",
    "unist-util-visit": "5.0.0",
    "vlq": "2.0.4"
  },
  "renovate": {
    "extends": [
      "github>nlibjs/renovate-config"
    ],
    "ignoreDeps": [
      "next"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@nlib/githooks": "0.2.0"
  }
}
