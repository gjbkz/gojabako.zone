{
  "name": "@gjbkz/gojabako.zone",
  "homepage": "https://gojabako.zone/",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-s test:*",
    "test:type": "tsc --noEmit",
    "test:unit": "tsm-test src",
    "dev": "run-p build:meta dev:*",
    "dev:css": "node --experimental-loader=@nlib/tsm src/cli/scssTypes.mts --watch",
    "dev:stories": "node --experimental-loader=@nlib/tsm src/cli/stories.mts --watch",
    "dev:next": "NODE_OPTIONS='--experimental-loader=@nlib/tsm' next",
    "https": "run-p build:meta dev:css dev:stories https:*",
    "https:next": "node --experimental-loader=@nlib/tsm server.mts",
    "build": "run-s build:*",
    "build:meta": "run-p build:meta:*",
    "build:meta:pageList": "node --experimental-loader=@nlib/tsm src/cli/pageList.mts",
    "build:meta:fonts": "node --experimental-loader=@nlib/tsm src/cli/fonts.mts",
    "build:meta:scssTypes": "node --experimental-loader=@nlib/tsm src/cli/scssTypes.mts",
    "build:meta:stories": "node --experimental-loader=@nlib/tsm src/cli/stories.mts",
    "build:next": "NODE_OPTIONS='--experimental-loader=@nlib/tsm' next build",
    "start": "NODE_ENV=production NODE_OPTIONS='--experimental-loader=@nlib/tsm' node server.mjs",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src *.mjs",
    "lint:css": "stylelint 'src/**/*.scss'"
  },
  "dependencies": {
    "@fontsource/noto-sans-jp": "5.0.19",
    "@mdx-js/loader": "3.0.1",
    "@next/mdx": "14.2.2",
    "@nlib/eslint-config": "3.20.5",
    "@nlib/githooks": "0.2.0",
    "@nlib/lint-commit": "1.0.1",
    "@nlib/tsm": "0.1.1",
    "@nlib/typing": "1.1.0",
    "@storybook/csf": "0.1.4",
    "@types/acorn": "6.0.0",
    "@types/kuromoji": "0.1.3",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.0",
    "acorn": "8.11.3",
    "chokidar": "3.6.0",
    "esbuild-loader": "4.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.2",
    "hast-util-from-html": "2.0.1",
    "hast-util-to-string": "3.0.0",
    "kuromoji": "0.1.2",
    "lint-staged": "15.2.2",
    "lowlight": "3.1.0",
    "mdast-util-from-markdown": "2.0.0",
    "mdast-util-to-hast": "13.1.0",
    "mdast-util-to-string": "4.0.0",
    "next": "14.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-scss": "4.0.9",
    "postcss-selector-parser": "6.0.16",
    "prettier": "3.2.5",
    "recoil": "0.7.7",
    "rehype-highlight": "7.0.0",
    "rehype-katex": "7.0.0",
    "rehype-parse": "9.0.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "remark-math": "6.0.0",
    "sass": "1.75.0",
    "sharp": "0.33.3",
    "stylelint": "16.3.1",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.4.5",
    "unist-util-find": "3.0.0",
    "unist-util-visit": "5.0.0",
    "vlq": "2.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.*(m|c)+(j|t)s*(x)": "eslint",
    "**/*.*(s)css": "stylelint"
  },
  "renovate": {
    "extends": [
      "github>nlibjs/renovate-config"
    ],
    "ignoreDeps": [
      "next"
    ]
  }
}
